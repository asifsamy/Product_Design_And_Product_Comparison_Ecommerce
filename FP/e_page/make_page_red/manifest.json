{
  "name": "E-Commerce Expert",
  "description": "Suggest Best Price",
  "version": "1.0",
  "permissions": [
    "activeTab",
     "tabs",
      "http://*/*", "https://*/*"
  ],
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["background.js"],
		"css": ["bg.css"]
      }
    ],
  "browser_action": {
    "default_title": "Price Show"
  },
  "manifest_version": 2
}
